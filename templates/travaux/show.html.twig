{% extends 'base.html.twig' %}

{% block title %}Travaux{% endblock %}

{% block body %}
    <h1>Travaux</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ travaux.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ travaux.description }}</td>
            </tr>
            <tr>
                <th>PrixUnitaire</th>
