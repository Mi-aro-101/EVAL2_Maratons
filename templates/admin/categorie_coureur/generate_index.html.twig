{% extends 'admin/base.html.twig' %}

{% block title %}Categorie coureur{% endblock %}

{% block body %}

<div class="row">
    <div class="col-md-6">
        <div class="card-body">
            {% for type, message in app.flashes %}
                <div class="alert alert-{{ type }}">
                {{ message| join('. ') }}
                </div>
            {% endfor %}
            <h4 class="card-title">Generation de categorie de coureur</h4>
            <div class="template-demo">
                <a href="{{ path('app_generate_categorie') }}"><button type="button" class="btn btn-warning">Generer categorie</button></a>
            </div>
        </div>
    </div>
</div>

{% endblock %}