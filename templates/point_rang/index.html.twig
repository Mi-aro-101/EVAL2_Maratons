{% extends 'base.html.twig' %}

{% block title %}PointRang index{% endblock %}

{% block body %}
    <h1>PointRang index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Point</th>
                <th>Rang</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for point_rang in point_rangs %}
            <tr>
                <td>{{ point_rang.id }}</td>
                <td>{{ point_rang.point }}</td>
                <td>{{ point_rang.rang }}</td>
                <td>
                    <a href="{{ path('app_point_rang_show', {'id': point_rang.id}) }}">show</a>
                    <a href="{{ path('app_point_rang_edit', {'id': point_rang.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_point_rang_new') }}">Create new</a>
{% endblock %}
